{
  "name": "usedapp",
  "private": true,
  "engines": {
    "node": ">=10",
    "yarn": "^1.17.3"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "lint": "wsrun -c -s lint",
    "lint:fix": "wsrun -c -s lint:fix",
    "build": "wsrun -e -c -s build",
    "test": "wsrun -e -c -s --exclude-missing test",
    "release": "yarn build && yarn changeset publish"
  },
  "dependencies": {
    "@changesets/cli": "^2.14.1",
    "ethers": "5.4.1",
    "prettier": "2.1.2",
    "tsconfig.json": "^1.0.10",
    "wsrun": "^5.2.1"
  },
  "resolutions": {
    "ethers": "5.4.1"
  },
  "packageManager": "yarn@3.1.0"
}
